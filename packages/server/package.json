{
  "name": "@bespoke/server",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "NEST_DEBUG nest start",
    "dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "codegen": "graphql-codegen --config codegen.ts",
    "typecheck": "tsc --noEmit",
    "typeorm": "ts-node ./node_modules/typeorm/cli",
    "typeorm:generate-migration": "npm run typeorm -- -d ./typeOrm.config.ts migration:generate ./migrations/$npm_config_name",
    "typeorm:revert-migration": "npm run typeorm -- -d ./typeOrm.config.ts migration:revert"
  },
  "dependencies": {
    "@apollo/server": "^4.7.1",
    "@aws-sdk/client-ses": "^3.342.0",
    "@aws-sdk/credential-provider-node": "^3.342.0",
    "@bespoke/common": "^0.0.1",
    "@bull-board/api": "^5.1.2",
    "@bull-board/express": "^5.1.2",
    "@liaoliaots/nestjs-redis": "^9.0.5",
    "@nestjs-shopify/auth": "4.0.0",
    "@nestjs-shopify/core": "^3.0.0",
    "@nestjs-shopify/webhooks": "^3.0.0",
    "@nestjs/apollo": "^11.0.6",
    "@nestjs/bull": "^0.6.3",
    "@nestjs/common": "^9.4.2",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.4.2",
    "@nestjs/event-emitter": "^1.4.1",
    "@nestjs/graphql": "^11.0.6",
    "@nestjs/platform-express": "^9.4.2",
    "@nestjs/schedule": "^2.2.2",
    "@nestjs/typeorm": "^9.0.1",
    "@ntegral/nestjs-sentry": "^4.0.0",
    "@sendgrid/client": "^7.7.0",
    "@sendgrid/eventwebhook": "^7.7.0",
    "@sendgrid/mail": "^7.7.0",
    "@sentry/hub": "^7.53.1",
    "@sentry/node": "^7.53.1",
    "@shopify/shopify-api": "^6.2.0",
    "aws-sdk": "^2.1387.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "bull": "^4.10.4",
    "class-validator": "^0.14.0",
    "cloudinary": "^1.37.0",
    "connect-redis": "^7.0.1",
    "cookie": "^0.5.0",
    "cookie-parser": "^1.4.6",
    "cookie-signature": "^1.2.1",
    "cors": "^2.8.5",
    "currency-symbol-map": "^5.1.0",
    "dayjs": "^1.11.7",
    "express-session": "^1.17.3",
    "graphql": "^16.6.0",
    "graphql-scalars": "^1.22.1",
    "handlebars": "^4.7.7",
    "helmet": "^6.1.5",
    "ioredis": "^5.3.2",
    "isemail": "^3.2.0",
    "json-bigint-patch": "^0.0.8",
    "mailparser": "^3.6.4",
    "mjml": "^4.14.1",
    "multer": "^1.4.5-lts.1",
    "namor": "^3.0.1",
    "nanoid": "3.3.4",
    "nodemailer": "^6.9.3",
    "nodemailer-html-to-text": "^3.2.0",
    "path-to-regexp": "^6.2.1",
    "pg": "^8.10.0",
    "posthog-node": "^3.1.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.1",
    "rxjs": "^7.8.1",
    "stripe": "^12.7.0",
    "tiny-invariant": "^1.3.1",
    "ts-morph": "^18.0.0",
    "typeorm": "^0.3.15",
    "yarn": "^1.22.19"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^4.0.0",
    "@graphql-codegen/typescript": "^4.0.0",
    "@graphql-codegen/typescript-document-nodes": "^4.0.0",
    "@graphql-codegen/typescript-operations": "^4.0.0",
    "@nestjs/cli": "^9.5.0",
    "@nestjs/schematics": "^9.2.0",
    "@nestjs/testing": "^9.4.2",
    "@shopify/cli": "^3.46.1",
    "@types/bcryptjs": "^2.4.2",
    "@types/bull": "^4.10.0",
    "@types/cookie": "^0.5.1",
    "@types/cookie-parser": "^1.4.3",
    "@types/cookie-signature": "^1.1.0",
    "@types/cors": "^2.8.13",
    "@types/cron": "^2.0.1",
    "@types/express": "^4.17.17",
    "@types/express-session": "^1.17.7",
    "@types/jest": "29.5.1",
    "@types/mailparser": "^3.4.0",
    "@types/mjml": "^4.7.1",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.2.5",
    "@types/nodemailer": "^6.4.8",
    "@types/nodemailer-html-to-text": "^3.1.0",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "29.5.0",
    "prettier": "^2.8.8",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "^4.9.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "workspaces": {
    "nohoist": [
      "**/@nestjs",
      "**/@nestjs/**",
      "**/@nestjs-shopify",
      "**/@nestjs-shopify/**",
      "**/dayjs/",
      "**/dayjs/**",
      "**/graphql-scalars",
      "**/graphql-scalars/**",
      "**/@liaoliaots/nestjs-redis",
      "**/@liaoliaots/nestjs-redis/**"
    ]
  }
}
