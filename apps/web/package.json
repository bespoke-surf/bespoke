{
  "name": "@bespoke/web",
  "version": "0.0.1",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "remix build --sourcemap && gen:scripts && yarn sentry-upload && yarn upload-assets",
    "sentry-upload": "is-ci && yarn sentry-upload-sourcemaps --release $npm_package_version && yarn clean-sourcemap",
    "clean-sourcemap": "node ./deleteSourcemaps.js",
    "upload-assets": "node ./uploadToS3.js",
    "dev": "remix dev",
    "start": "remix-serve build",
    "codegen": "graphql-codegen --config codegen.yml",
    "gen:form": "tsup --env.FRONTEND_HOST $FRONTEND_HOST --env.BACKEND_HOST $BACKEND_HOST",
    "gen:scripts": "dotenv -- yarn gen:form",
    "typecheck": "tsc --noEmit",
    "eslint": "eslint --fix-dry-run"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.357.0",
    "@bespoke/common": "^0.0.1",
    "@clayui/css": "^3.98.0",
    "@clayui/slider": "^3.96.0",
    "@lexical/code": "^0.11.1",
    "@lexical/hashtag": "^0.11.1",
    "@lexical/html": "^0.11.1",
    "@lexical/link": "^0.11.1",
    "@lexical/list": "^0.11.1",
    "@lexical/mark": "^0.11.1",
    "@lexical/overflow": "^0.11.1",
    "@lexical/react": "^0.11.1",
    "@lexical/rich-text": "^0.11.1",
    "@lexical/selection": "^0.11.1",
    "@lexical/table": "^0.11.1",
    "@lexical/utils": "^0.11.1",
    "@remix-run/node": "^1.17.1",
    "@remix-run/react": "^1.17.1",
    "@remix-run/serve": "^1.17.1",
    "@remix-validated-form/with-yup": "^2.0.3",
    "@sentry/remix": "^7.56.0",
    "@shopify/address": "^4.1.0",
    "@uppy/core": "^3.2.1",
    "@uppy/dashboard": "^3.4.1",
    "@uppy/drag-drop": "^3.0.2",
    "@uppy/file-input": "^3.0.2",
    "@uppy/progress-bar": "^3.0.2",
    "@uppy/react": "^3.1.2",
    "@uppy/xhr-upload": "^3.3.0",
    "@welldone-software/why-did-you-render": "^7.0.1",
    "axios": "^1.4.0",
    "cheerio": "^1.0.0-rc.12",
    "cross-fetch": "^3.1.5",
    "currency-symbol-map": "^5.1.0",
    "dagre": "^0.8.5",
    "dayjs": "^1.11.8",
    "formik": "^2.4.2",
    "gestalt": "^111.5.12",
    "gestalt-datepicker": "^111.5.12",
    "graphql": "^16.7.0",
    "graphql-tag": "^2.12.6",
    "immer": "^9.0.19",
    "isbot": "^3.6.12",
    "lexical": "^0.11.1",
    "lodash": "^4.17.21",
    "millify": "^6.1.0",
    "papaparse": "^5.4.1",
    "posthog-js": "^1.68.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-intersection-observer": "^9.5.1",
    "react-textarea-autosize": "^8.5.0",
    "react-use-clipboard": "^1.0.9",
    "react-virtual": "^2.10.4",
    "react-waypoint": "^10.3.0",
    "reactflow": "^11.7.3",
    "remix-etag": "^1.0.0",
    "remix-params-helper": "^0.4.10",
    "remix-utils": "^6.4.1",
    "remix-validated-form": "^4.6.12",
    "slug": "^8.2.2",
    "suspend-concurrently": "^0.5.0",
    "tiny-invariant": "^1.3.1",
    "tsup": "^6.7.0",
    "yjs": "^13.6.4",
    "yup": "^0.32.11",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^4.0.1",
    "@graphql-codegen/typescript": "^4.0.1",
    "@graphql-codegen/typescript-generic-sdk": "^3.1.0",
    "@graphql-codegen/typescript-operations": "^4.0.1",
    "@remix-run/dev": "^1.17.1",
    "@remix-run/eslint-config": "^1.17.1",
    "@swc/core": "^1.3.66",
    "@types/css": "^0.0.33",
    "@types/dagre": "^0.7.48",
    "@types/gestalt": "^101.0.0",
    "@types/gestalt-datepicker": "^55.2.1",
    "@types/jsdom": "^21.1.1",
    "@types/papaparse": "^5.3.7",
    "@types/react": "^18.2.13",
    "@types/react-dom": "^18.2.6",
    "@types/react-email-editor": "^1.5.1",
    "@types/slug": "^5.0.3",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "dotenv-cli": "^7.2.1",
    "eslint": "^8.43.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-gestalt": "^111.5.12",
    "lerna": "^6.6.2",
    "remix-flat-routes": "^0.5.8",
    "rimraf": "^5.0.1",
    "s3-folder-upload": "^2.3.5",
    "typescript": "^4.9.5"
  },
  "engines": {
    "node": ">=14"
  },
  "tsup": {
    "entry": [
      "scripts/bespoke.ts"
    ],
    "outDir": "public/js",
    "clean": true,
    "format": [
      "iife"
    ],
    "splitting": false,
    "sourcemap": false,
    "bundle": true,
    "legacyOutput": false,
    "watch": false,
    "target": "es5",
    "minify": true,
    "keepNames": false,
    "treeshake": true
  }
}
